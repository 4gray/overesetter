{
    "productName": "oversetter",
    "directories": {
        "output": "app-builds"
    },
    "files": [
        "**/*",
        "!*.ts",
        "!*.code-workspace",
        "!LICENSE.md",
        "!package.json",
        "!package-lock.json",
        "!src/",
        "!e2e/",
        "!hooks/",
        "!.angular-cli.json",
        "!_config.yml",
        "!tsconfig.json",
        "!tslint.json",
        "!src/"
    ],
    "win": {
        "icon": "assets",
        "target": ["portable"]
    },
    "mac": {
        "icon": "assets",
        "target": ["dmg"]
    },
    "linux": {
        "icon": "assets",
        "target": ["AppImage", "Snap"]
    }
}
